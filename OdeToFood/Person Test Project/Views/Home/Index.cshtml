@model  EmployeeTest.ViewModels.HomeIndexViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <h1>@Model.message</h1>
    <table>
        @foreach (var employee in Model.employees)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Id</td>
            </tr>
        }
    </table>

</body>
</html>